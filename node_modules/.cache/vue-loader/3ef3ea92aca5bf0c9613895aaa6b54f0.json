AACA;AACA;;AAEA;AACA;AACA","file":"HelloWorld.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div>\n    \n    \n    <!-- <VuePerfectScrollbar class=\"scroll-area\"> -->\n    <!-- <p>lorem5</p> -->\n    <div class=\"flexWrapper\" style=\"\">\n      <!-- random text -->\n      <el-card class=\"box-card\" v-for=\"(cards, index) in newsHeadlines\">\n        <img :src=\"imageHref[index]\" style=\"width: 100%; height: 200px\">\n        <div><a :href=\"newsLinkHref[index]\" target=\"new\">{{ newsHeadlines[index] }}</a></div>\n      </el-card>\n    <!-- <infinite-loading @infinite=\"infiniteHandler\"></infinite-loading> -->\n    </div>\n    <infinite-loading @infinite=\"infiniteHandler\"></infinite-loading>\n    <!-- </VuePerfectScrollbar> -->\n\n    \n    <!-- <infinite-loading @infinite=\"infiniteHandler\"></infinite-loading>  -->\n\n    \n  </div>\n</template>\n\n<script>\n\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar';\n\n\n\n\n\n\nexport default {\n\n  name: 'HelloWorld',\n  props: {\n    // msg: String\n  },\n  data() {\n      return {\n        scrollState: '',\n        newsHeadlines: [],\n        imageHref: [],\n        newsLinkHref: [],\n        value: '',\n        pageNumber: 0,\n        \n\n      }\n    },\n  methods: {\n    infiniteHandler($state) {\n      // this.fetchnews();\n      console.log(\"called\");\n      ++this.pageNumber;\n      console.log(this.pageNumber);\n      \n      if (this.pageNumber === 2){\n        this.fetchNews($state);\n        $state.complete()\n      }\n      else {\n        this.fetchNews($state);\n      }\n      this.scrollState = $state;\n\n\n      // $state.loaded();\n      // $state.complete();\n    },\n    fetchNews ($state) {\n      \n      // this.$api.get('https://newsapi.org/v2/top-headlines?country=in&page=' + this.pageNumber +'&apiKey=3508a47b6f494cc1986ad217c87cb65f') \n      this.$api.get('https://newsapi.org/v2/everything?q=Dhoni&from=2019-03-27&sortBy=publishedAt&page=5&apiKey=3508a47b6f494cc1986ad217c87cb65f')\n      .then(response => {\n        // $state.loaded();\n      console.log(response);\n      for (let i = 0; i < 20; i++) {\n        this.newsHeadlines.push(response.data.articles[i].title);\n        this.imageHref.push(response.data.articles[i].urlToImage);\n        this.newsLinkHref.push(response.data.articles[i].url);\n\n        // console.log(response.data.articles[i].title);\n      };\n      \n      \n      $state.loaded();\n      console.log($state);\n    })\n    .catch(e => {\n      // this.errors.push(e)\n      console.log(e);\n    })\n    \n    \n  }\n  },\n  mounted() {\n    // this.fetchNews();\n  },\n  components: {\n    VuePerfectScrollbar,\n\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n.el-dropdown-link {\n    cursor: pointer;\n    color: #409EFF;\n  }\n.el-icon-arrow-down {\n    font-size: 12px;\n  }\n  .text {\n    font-size: 14px;\n  }\n\n  .item {\n    padding: 18px 0;\n  }\n\n  .box-card {\n    width: 45%; \n    display: inline-block; \n    margin-bottom: 3%\n  }\n  a {\n    text-decoration: none;\n    color: #409eff;\n  }\n\n  a:hover {\n    color: #1989fa;\n  }\n  \n  .flexWrapper {\n    display: flex; \n    flex-wrap: wrap; \n    justify-content: space-around;\n  }\n\n.scroll-area {\n  position: relative;\n  margin: auto;\n  width: 98vw;\n  height: 90vh;\n  }\n\n.el-card__body {\n    padding: 0;\n  }\n\n  \n</style>\n"]}]}